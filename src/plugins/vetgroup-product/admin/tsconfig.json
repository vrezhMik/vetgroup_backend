{
  "extends": "@strapi/typescript-utils/tsconfigs/admin",
  "include": ["src", "custom.d.ts"],
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "../dist/admin",
    "declaration": true,
    "emitDeclarationOnly": false,
    "noEmit": false,
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
